#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hp-elitebook/projects/dummy-project/backend/node_modules/.pnpm/typeorm@0.3.25_ioredis@5.7.0_pg@8.16.3_redis@5.8.0_reflect-metadata@0.2.2_ts-node@10.9._501f34ad6f8cd407dd37dd59c9dacdee/node_modules/typeorm/node_modules:/home/hp-elitebook/projects/dummy-project/backend/node_modules/.pnpm/typeorm@0.3.25_ioredis@5.7.0_pg@8.16.3_redis@5.8.0_reflect-metadata@0.2.2_ts-node@10.9._501f34ad6f8cd407dd37dd59c9dacdee/node_modules:/home/hp-elitebook/projects/dummy-project/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hp-elitebook/projects/dummy-project/backend/node_modules/.pnpm/typeorm@0.3.25_ioredis@5.7.0_pg@8.16.3_redis@5.8.0_reflect-metadata@0.2.2_ts-node@10.9._501f34ad6f8cd407dd37dd59c9dacdee/node_modules/typeorm/node_modules:/home/hp-elitebook/projects/dummy-project/backend/node_modules/.pnpm/typeorm@0.3.25_ioredis@5.7.0_pg@8.16.3_redis@5.8.0_reflect-metadata@0.2.2_ts-node@10.9._501f34ad6f8cd407dd37dd59c9dacdee/node_modules:/home/hp-elitebook/projects/dummy-project/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/typeorm@0.3.25_ioredis@5.7.0_pg@8.16.3_redis@5.8.0_reflect-metadata@0.2.2_ts-node@10.9._501f34ad6f8cd407dd37dd59c9dacdee/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../.pnpm/typeorm@0.3.25_ioredis@5.7.0_pg@8.16.3_redis@5.8.0_reflect-metadata@0.2.2_ts-node@10.9._501f34ad6f8cd407dd37dd59c9dacdee/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
fi
